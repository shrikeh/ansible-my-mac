---
ssh_config_path: ~/.ssh/config2
ssh_config_overwrite_file: yes
ssh_config_frontdoor2: 'ssh fd2session nc %h %p 2>/dev/null'
ssh_config_frontdoor_legacy: 'ssh frontdoor nc %h %p'

ssh_config_groups:
  - name: goldquest
    entries:
      - description: Goldquest
        hosts:
        - secure.goldquestsoftware.com
        vars:
          - "ProxyCommand {{ ssh_config_frontdoor2 }}"
  - name: acne
    entries:
    - description: Acne UAT
      hosts:
      - acne-uat
      - uat.acnestudios.com
      vars:
        - "Hostname 94.229.168.32"
        - Port 2020
        - "ProxyCommand {{ ssh_config_frontdoor_legacy }}"
  - name: melissa-odabash
    entries:
      - description: Melissa Odabash
        hosts:
        - 89.167.216.192
        - 89.167.216.188
        vars:
          - "IdentityFile {{ ssh_config_keys_path.sessiondigital }}/melissa/melissa_rsa"
          - "Port 2211"
          - "ProxyCommand {{ ssh_config_frontdoor2 }}"

  - name: github
    entries:
      - description: Github
        hosts:
          - github.com
        vars:
          - Hostname github.com
          - "IdentityFile {{ ssh_config_keys_path.personal }}/github/github_rsa"
          - "UserKnownHostsFile {{ ssh_config_hosts.personal }}/github"
  - name: missguided
    description: Missguided
    entries:
      - hosts:
          - web01.missguided.co.uk
        vars:
          - Hostname 172.30.134.44
      - hosts:
          - web02.missguided.co.uk
        vars:
          - Hostname 172.30.134.45
      - hosts:
          - web03.missguided.co.uk
        vars:
          - Hostname 172.30.134.46
      - hosts:
          - web04.missguided.co.uk
        vars:
          - Hostname 172.30.134.47
      - hosts:
          - web05.missguided.co.uk
        vars:
          - Hostname 172.30.134.48
      - hosts:
          - web06.missguided.co.uk
        vars:
          - Hostname 172.30.134.49
      - hosts:
          - admin01.missguided.co.uk
        vars:
          - Hostname 172.30.134.40
      - hosts:
          - admin02.missguided.co.uk
        vars:
          - Hostname 172.30.134.41
      - hosts:
          - varnish01.missguided.co.uk
        vars:
          - Hostname 172.30.134.42
      - hosts:
          - varnish02.missguided.co.uk
        vars:
          - Hostname 172.30.134.43
      - hosts:
          - cache01.missguided.co.uk
        vars:
          - Hostname 172.30.134.38
      - hosts:
          - cache02.missguided.co.uk
        vars:
          - Hostname 172.30.134.39
      - hosts:
          - nfs01.missguided.co.uk
        vars:
          - Hostname 172.30.134.6
      - hosts:
          - nfs02.missguided.co.uk
        vars:
          - Hostname 172.30.134.7
      - hosts:
          - db01.missguided.co.uk
        vars:
          - Hostname 172.30.134.9
      - hosts:
          - db02.missguided.co.uk
        vars:
          - Hostname 172.30.134.10
      - hosts:
          - db03.missguided.co.uk
        vars:
          - Hostname 172.30.134.7
      - hosts:
          - db04.missguided.co.uk
        vars:
          - Hostname 172.30.134.7
      - hosts:
          - db05.missguided.co.uk
        vars:
          - Hostname 172.30.134.7
      - hosts:
          - db06.missguided.co.uk
        vars:
          - Hostname 172.30.134.7
      - hosts:
          - '*missguided*'
          - '172.30.134.*'
          - 'mg.*'
        vars:
          - "UserKnownHostsFile {{ ssh_config_hosts.sessiondigital }}/missguided"
          - "IdentityFile {{ ssh_config_keys_path.sessiondigital }}/missguided/missguided_rsa"
  - name: radley
    description: Radley
    entries:
      - hosts:
          - '*radley*'
          - '162.13.14.*'
        vars:
          - "UserKnownHostsFile {{ ssh_config_hosts.sessiondigital }}/radley"
          - "IdentityFile {{ ssh_config_keys_path.sessiondigital }}/radley/radley_rsa"
  - name: inviqa
    entries:
      - hosts:
          - fd2session
          - frontdoor2.sessiondigital.com
        vars:
          - "ForwardAgent yes"
          - "UserKnownHostsFile {{ ssh_config_hosts.sessiondigital }}/frontdoor"
          - "IdentityFile {{ ssh_config_keys_path.sessiondigital }}/fd2session/fd2session_rsa"
      - hosts:
        - frontdoor
        vars:
          - 'Hostname frontdoor.inviqa.com'
          - "Port 2222"
          # prevents the connection to the server to be reset
          - "ServerAliveInterval 20"
          # add here any LocalForward directives you may need
          # allows the redirection of ldap port service to run queries for debug purposes, or to use local LDAP frontend applications
          - 'LocalForward 8389: fitch:389'
            # allows a direct access to Adminer for MySQL webmanagemnt
          - 'LocalForward 8888: forfeit-horn:80'
      - hosts:
          - '*.inviqa.com'
        vars:
          - "UserKnownHostsFile {{ ssh_config_hosts.inviqa }}/default"
      - hosts:
          - '*.sessiondigital.com'
        vars:
          - "UserKnownHostsFile {{ ssh_config_hosts.sessiondigital }}/default"
      - hosts:
        - '*.frontdoor.inviqa.com'
        vars:
          - "ProxyCommand {{ ssh_config_frontdoor_legacy }}"
      - hosts:
          - '!fd2session'
          - '!frontdoor'
          - '*.sessiondigital.com'
          - '*.phplab.co.uk'
        vars:
          - "ProxyCommand {{ ssh_config_frontdoor2 }}"
          - "IdentityFile {{ ssh_config_keys_path.sessiondigital }}/session_default/session_default_rsa"
  - name: digital-ocean
    entries:
      - hosts:
          - '178.62.*'
          - '46.101.*'
          - '104.236.*'
        vars:
          - "IdentityFile {{ ssh_config_keys_path.personal }}/digital_ocean/digital_ocean_rsa"
          - StrictHostKeyChecking no
          - UserKnownHostsFile /dev/null
  - name: rackspace-cloud
    entries:
      - hosts:
          - '92.52.*'
        vars:
          - 'ProxyCommand ssh fd2session nc %h %p 2>/dev/null'
          - "IdentityFile {{ ssh_config_keys_path.sessiondigital }}/session_default/session_default_rsa"
          - StrictHostKeyChecking no
          - UserKnownHostsFile /dev/null
  - name: holiday-weather
    entries:
      - hosts:
        - 134.213.75.207
        - 164.177.135.8
        - 83.138.162.221
        vars:
          - "ProxyCommand {{ ssh_config_frontdoor2 }}"
          - "IdentityFile {{ ssh_config_keys_path.inviqa }}/holiday_weather/holiday_weather_rsa"
  - name: default
    entries:
      - hosts:
          - '*'
        vars:
          - ForwardAgent no
          - TCPKeepAlive no
          - ServerAliveInterval 300
          - ServerAliveCountMax 10
          - GSSAPIAuthentication no
          - GSSAPIDelegateCredentials no
          - "User {{ ssh_config_default_user }}"
          - Compression yes
          - VisualHostKey yes
          - "IdentityFile {{ ssh_config_keys_path.sessiondigital }}/session_default/session_default_rsa"

...
